{
  "name": "PlayPlay-Rdio",
  "description": "Global shortcut keys for Rdio",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["http://www.rdio.com/*"],
      "js": ["playplay_client.js" ]
    }, {
      "matches": ["http://cmd.fm/*"],
      "js": ["playplay_cmdfm.js" ]
    }
  ],

  "permissions": [
    "http://www.rdio.com/"
  ],

  "manifest_version": 2
}
